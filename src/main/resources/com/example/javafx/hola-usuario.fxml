<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="387.0" prefWidth="534.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.HolaUsuario">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>

   <Label fx:id="etiqueta" textAlignment="CENTER">
      <font>
         <Font size="24.0" />
      </font></Label>
   <HBox alignment="CENTER" prefHeight="39.0" prefWidth="434.0">
      <children>
         <Label prefHeight="27.0" prefWidth="72.0" text="Nombre">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="textField" prefHeight="39.0" prefWidth="173.0">
            <font>
               <Font size="18.0" />
            </font>
         </TextField>
      </children>
   </HBox>
   <Button fx:id="button" alignment="CENTER" contentDisplay="CENTER" onAction="#botonClick" text="Click" textAlignment="CENTER">
      <font>
         <Font size="20.0" />
      </font></Button>
</VBox>
